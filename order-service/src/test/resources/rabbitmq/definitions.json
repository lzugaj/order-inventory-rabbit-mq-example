{
  "rabbit_version": "3.13.7",
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "order-created-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "order-failed-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "order-confirmed-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "ecommerce.order.topic-exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "ecommerce.order.topic-exchange",
      "vhost": "/",
      "destination": "order-created-queue",
      "destination_type": "queue",
      "routing_key": "order.created",
      "arguments": {}
    },
    {
      "source": "ecommerce.order.topic-exchange",
      "vhost": "/",
      "destination": "order-failed-queue",
      "destination_type": "queue",
      "routing_key": "order.failed",
      "arguments": {}
    },
    {
      "source": "ecommerce.order.topic-exchange",
      "vhost": "/",
      "destination": "order-confirmed-queue",
      "destination_type": "queue",
      "routing_key": "order.confirmed",
      "arguments": {}
    }
  ]
}
